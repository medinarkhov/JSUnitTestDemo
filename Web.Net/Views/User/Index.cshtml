@model Web.Net.Models.UserModel

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>MVC App</title>
</head>
<body>
    <div>
        <h3>@Model.Message</h3>
        <ul>
            @foreach (var user in Model.Users)
            {
                <li>@user</li>
            }
        </ul>
    </div>
</body>
</html>